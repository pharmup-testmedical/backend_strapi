{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Товар"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "canonicalName": {
      "type": "string",
      "required": true,
      "unique": true,
      "displayName": "Canonical name"
    },
    "cashbackEligible": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "category": {
      "type": "string"
    },
    "productAliases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-alias.product-alias",
      "mappedBy": "product"
    }
  }
}
