{
  "kind": "collectionType",
  "collectionName": "cashback_requests",
  "info": {
    "singularName": "cashback-request",
    "pluralName": "cashback-requests",
    "displayName": "Вывод средств",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "verificationStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved",
        "rejected",
        "manual_review"
      ]
    },
    "requester": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "cashbackRequests"
    },
    "amount": {
      "type": "decimal",
      "required": true,
      "default": 500
    }
  }
}
